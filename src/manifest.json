{
  "manifest_version": 2,
  "version": "1.0.0",
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "author": "David Sferruzza",
  "default_locale": "en",
  "permissions": ["dns", "storage", "webNavigation", "activeTab", "<all_urls>"],
  "icons": {
    "48": "../assets/unknown.svg",
    "96": "../assets/unknown.svg"
  },
  "background": {
    "scripts": ["background.ts"],
    "persistent": false
  },
  "web_accessible_resources": [
    "_locales/**/messages.json",
    "../assets/no.svg",
    "../assets/unknown.svg",
    "../assets/yes.svg"
  ],
  "page_action": {
    "default_icon": "../assets/unknown.svg"
  }
}
